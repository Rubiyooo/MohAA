TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/lights/tiffanylamp
	skelmodel tiffanylamp.skd
	surface tiffanyshade shader static_tiffanyshade
	surface lampbase shader static_lampbase
}

animations
{
	idle    tiffanylamp.skc
}

