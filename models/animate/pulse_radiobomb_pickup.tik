TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/items/explosive
	skelmodel radiobomb.skd
	surface radiobomb shader pulsating_radiobomb_pickup
}

init
{
	server
	{
		classname animate
	}
}

animations
{
	idle     radiobomb.skc
}

/*QUAKED animate_item_radiobomb-pickup-pulsating (0.5 0.5 1.0) (0 0 0) (0 0 0)
Benson Russell 10-16-2001 - pulsating radio bomb for picking up in the level
*/