TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/items/explosive
	skelmodel radiobomb.skd
	surface radiobomb shader pulsating_radiobomb
}

init
{
	server
	{
		classname animate
	}
}

animations
{
	idle     radiobomb.skc
}

