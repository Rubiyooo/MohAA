TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/miscobj/radio
	skelmodel german_radio.skd
	surface german_radio1 shader radio_military_pulsating
	surface german_radio2 shader radio_military_pulsating
}

init
{
	server
	{
		classname animate
	}
}

animations
{
	idle     german_radio.skc
}

