TIKI
setup
{
	scale 1.0
	path models/fx/dummy
	skelmodel dummy2.skd
}
 
init
{
	server  
	{
		classname Explosion
		rendereffects +dontdraw
		notsolid
		radiusdamage 300
		radius 450
		meansofdeath explosion
//		life 0.1
	}
}

animations
{
	idle dummy2.skc

	start dummy2.skc
	{
		server    
		{
			first damageagain
			last stopanimating
		}
	}
}

