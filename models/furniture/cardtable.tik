TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/furniture/table
	skelmodel anim_table.skd
	surface worktable shader static_worktable
	surface cardpile  shader cardpile
}

init
{
	server
	{
		classname animate
	}
}

animations
{
	idle     anim_table.skc
}

