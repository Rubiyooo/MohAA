	//scale 1.33			// Convert from inches to game units
	path models/miscobj/searchlight
	skelmodel searchlighton.skd
	surface searchlight shader searchlight
	surface beam shader beam
}

init
{
	server
	{
		classname animate
	}
}

animations
{
	idle     searchlighton.skc
}

