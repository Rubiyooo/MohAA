TIKI
setup
{
	scale 0.52
	path models/miscobj/clipboard_pencil
	skelmodel attached_clipboard.skd
	surface all shader clipboard
}

init
{
	server
	{
		classname interactobject
		setsize "-4 -4 0" "4 4 8"
		killedeffect "" // tiki to spawn when it's destroyed
		hiteffect "" // tiki to spawn when it's hit
	}
}

animations
{
	idle     attached_clipboard.skc
}

