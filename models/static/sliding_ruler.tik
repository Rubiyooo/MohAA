TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/sliding_ruler
	skelmodel sliding_ruler.skd
	surface sliding_ruler shader static_sliding_ruler
	surface sliding_ruler_alpha shader static_sliding_ruler_alpha

}

init
{
	server
	{
		classname object
	}
}

animations
{
	idle    sliding_ruler.skc
}

