TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/doublemetalbed
	skelmodel doublemetalbed.skd
	surface dmetalbed shader static_dmetalbed
	surface dmetalbedpost shader static_dmetalbedpost
}

animations
{
	idle     doublemetalbed.skc
}

