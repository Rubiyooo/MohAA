TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/vehicles/fockwulf
	skelmodel fockwulf.skd
	surface fockwulf3 shader static_fockwulf
	surface fockwindow shader static_fockwindow
	surface fockwulf1 shader static_fockwulf
}

animations
{
	idle     fockwulf.skc
}

