TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	//scale 1.33			// Convert from inches to game units
	path models/static/minepole
	skelmodel minepole.skd
	surface minepole shader static_minepole
	surface minepole2 shader static_minepole2
}

animations
{
	idle     minepole.skc
}

