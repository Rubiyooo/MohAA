TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/singlemetalbed
	skelmodel singlemetalbed.skd
	surface singlemetalbed shader static_singlemetalbed
	surface posts shader static_singlemetalbedpost
}

animations
{
	idle     singlemetalbed.skc
}

