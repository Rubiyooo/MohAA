TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/drawing_compass
	skelmodel drawing_compass.skd
	surface drawing_compass_1 shader static_drawing_compass_1
	surface drawing_compass_2 shader static_drawing_compass_2
}

animations
{
	idle    drawing_compass.skc
}

