TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/vehicles/car
	skelmodel car.skb
	surface carwindow shader static_carwindow
	surface car3 shader static_car
	surface car2 shader static_car
}

animations
{
	idle     car.skc
}

