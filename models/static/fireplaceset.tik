TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/fireplaceset
	skelmodel fireplaceset.skd
	surface cradle shader static_fireplacecradle
	surface wood shader static_fireplacewood
	surface poker shader static_poker
	surface stand shader static_stand
}

animations
{
	idle     fireplaceset.skc
}

