TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/submodels
	skelmodel ruddergauge.skd
	surface material1 shader static_ruddergauge2
	surface material2 shader static_ruddergaugebody
	surface material3 shader static_blackgauge
	surface material4 shader static_glass

}

animations
{
	idle     ruddergauge.skc
}

