TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/vehicles/dtruck
	skelmodel dtruck.skd
	surface dtruckwindow shader static_dtruckwindow
	surface dtruck3 shader static_dtruck1
	surface dtruck2 shader static_dtruck1
}

animations
{
	idle     dtruck.skc
}

