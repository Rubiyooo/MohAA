TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/lamp
	skelmodel light2.skd
	surface support shader static_support_snow
     	surface lantern shader static_lantern_snow
	surface post shader static_t_post_winter
	surface s_cross shader static_s_cross_winter
}

animations
{
	idle     light2.skc
}

