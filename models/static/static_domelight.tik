TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/bunkerlights
	skelmodel domelight.skd
	surface fixture shader static_lightbulb
	surface lightbulb shader static_domelight
	surface glass shader static_glass
}

animations
{
	idle     domelight.skc
}

