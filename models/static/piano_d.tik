TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/piano
	skelmodel piano.skd
	surface piano shader static_piano_d
}

init
{
	server
	{
		classname animate
		setsize "-4 -4 0" "4 4 4"
//		seatoffset "-14 0 24"
//		gravity 1.5
//		vehiclespeed 800
	}
}

animations
{
	idle     piano.skc
}

