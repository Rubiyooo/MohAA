TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/grandfathers_clock
	skelmodel grandfathers_clock.skd
	surface grandfathers_clock shader grandfathers_clock_d
	surface grandfathers_clock_pendulum shader grandfathers_clock_pendulum_d
	surface grandfathers_clock_face shader grandfathers_clock_face_d
	surface grandfathers_clock_glass shader grandfathers_clock_glass_d
}

init
{
	server
	{
		classname object
	}
}

animations
{
	idle     grandfathers_clock.skc
}

